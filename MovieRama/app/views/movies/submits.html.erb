<div class="container">
	<div class="row">
		<div class="col-sm-12">
			<div class="submits-header">
				<h2><%= @user.first_name%> <%= @user.last_name %></h2>
			</div>
		</div>
		<div class="col-sm-10">			
			<% @user.movies.each do |movie| %>
				<div class="block">
					<h2><%= link_to(movie.title, movie_path(movie.id)) %></h2>
					<p><%= movie.created_at %></p>
					<p><%= movie.description %></p>
				</div>
			<% end %>			
		</div>
	</div>
</div>