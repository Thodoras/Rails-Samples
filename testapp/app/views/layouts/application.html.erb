<!DOCTYPE html>
<html>
<head>
  <title>Testapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class='top'>
  Testapp - User
</div>
<% if admin_signed_in?%>
	<%= link_to("Sign Out", destroy_admin_session_path, method: :delete)%>
	<%= link_to("Admin", admin_vessels_path)%>
<% else %>
	<%= link_to("Log In", new_admin_session_path) %> or
	<%= link_to("Sign Up", new_admin_registration_path, class: "btn btn-primary", id: "sign-up")%>
<% end %>
<%= yield %>

</body>
</html>
