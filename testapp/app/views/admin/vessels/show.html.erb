<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Daily price</th>
      <th>Fee %</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
      <tr>
        <td><%= @vessel.title %></td>
        <td><%= @vessel.description %></td>
        <td><%= number_to_currency(@vessel.daily_price) %></td>
        <td><%= number_to_percentage(@vessel.fee_pc) %></td>
        <td><%= link_to 'Edit', edit_admin_vessel_path(@vessel) %></td>
        <td><%= link_to 'Destroy', admin_vessel_path(@vessel), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
  </tbody>
</table>